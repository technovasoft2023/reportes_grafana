          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT a.nombre::Text \"CODIGO\",\n      a.fecha::Text \"FECHA CREACION\",\n    \te.cedula::Text \"CEDULA\",\n    \te.nombre::TEXT \"DENUNCIANTE\",\n    \te.telefono::Text \"TELEFONO\",\n    \te.telefono_alt::Text \"TELEFONO ALT\",\n    \te.correo::Text \"CORREO\",\n    \tb.nombre::Text \"ESTADO\",\n    \tc.nombre::Text \"MUNICIPIO\",\n    \td.nombre::Text \"PARROQ\",\n    \ta.direccion::Text \"DIRECCION\",\n      a.titulo::Text \"ASUNTO\",\n      a.telefono_suscrip::Text \"TELEF./NUMERO SUSCRIP\",\n    \ta.descripcion::TEXT \"DESCRIP\", \n\t  \tf.name::Text \"CATEGORIA\",\n\t  \t--g.nombre::Text \"SUBCATEG\",\n\t  \t--case when m.nombre is null then 'Sin ExtCateg' else m.nombre end::Text \"EXTCATEG\", \n\t  \th.fecha_registro::Text \"FECHA STATUS\", \n\t\t  i.nombre::Text \"ESTATUS\",\n\t\t  k.nombre::Text \"RESPONSABLE\"\n        FROM gestion.caso a\n            inner join gestion.estado b on b.id = a.id_estado\n            inner join gestion.municipio c on c.id = a.id_municipio\n            inner join gestion.parroquia d on d.id = a.id_parroquia\n            left join gestion.denunciante e on e.id = a.id_denunciante\n            inner join gestion.category f on f.id = a.id_categoria\n            --inner join gestion.sub_categoria g on g.id = a.id_sub_categoria\n            --left JOIN  gestion.extra_categoria m ON m.id = a.id_extra_categoria\n            inner JOIN  gestion.historico h ON h.id_caso = a.id\n            inner JOIN  gestion.estatus i ON h.id_estatus = i.id            \n            inner JOIN  gestion.responsable j ON j.id_caso = h.id_caso\n            inner JOIN  gestion.resolutorio k ON k.id = j.id_resolutorio\n      where  k.nombre in ($resolutor)\n      --j.id_resolutorio in (563,566,597)\n      and j.principal is true \n      --and j.id_user is null\n      --and h.active = 1\n      and h.id_estatus in (3,4,8) --limit 1000",
          "refId": "A",
          "select": [
            [
              {
                "params": [
                  "value"
                ],
                "type": "column"
              }
            ]
          ],
          "timeColumn": "time",
          "where": [
